        </chunk>
      </chunks>
      <transformations>
        <transformation>
          <type>x-shift</type>
          <offset>199</offset>
        </transformation>
        <transformation>
          <type>y-shift</type>
          <offset>-277</offset>
        </transformation>
      </transformations>
    </layer>

    <layer>
      <name>Wind direction</name>

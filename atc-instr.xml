        </chunk>
      </chunks>
      <transformations>
        <transformation>
          <type>x-shift</type>
          <offset>199</offset>
        </transformation>
        <transformation>
          <type>y-shift</type>
          <offset>-299</offset>
        </transformation>
      </transformations>
    </layer>

    <layer>
      <name>Wind speed</name>
      <type>text</type>
      <font>default</font>
      <point-size>12</point-size>
      <color>
        <red>0.6</red>
        <green>0.8</green>
        <blue>1</blue>
      </color>
      <chunks>
        <chunk>
          <type>number-value</type>
          <property>environment/wind-speed-kt</property>
          <format>%- 2.0f</format>
        </chunk>
      </chunks>
      <transformations>
        <transformation>
          <type>x-shift</type>
          <offset>263</offset>
        </transformation>
        <transformation>
          <type>y-shift</type>
          <offset>-299</offset>
        </transformation>
      </transformations>
    </layer>


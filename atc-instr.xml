        <red>0.8</red>
        <green>0.8</green>
        <blue>0.8</blue>
      </color>
      <chunks>
        <chunk>
          <type>number-value</type>
          <property>/sim/atc/target-hdg</property>
          <format>%- 3.0f</format>
        </chunk>
      </chunks>
      <transformations>
        <transformation>
          <type>x-shift</type>
          <offset>199</offset>
        </transformation>
        <transformation>
          <type>y-shift</type>
          <offset>-224</offset>
        </transformation>
      </transformations>
    </layer>

    <layer>
      <name>radar range</name>
      <type>text</type>
      <font>default</font>
      <point-size>12</point-size>
      <color>
        <red>0.1</red>
        <green>0.8</green>
        <blue>0</blue>
      </color>
      <chunks>
        <chunk>
          <type>number-value</type>

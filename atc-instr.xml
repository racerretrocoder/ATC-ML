  </layers>

  <actions>
    <action>
      <name>previous mp</name>
      <type>adjust</type>
      <button>0</button>
      <x>109</x>
      <y>-194</y>
      <w>20</w>
      <h>20</h>
      <binding><command>dialog-close</command><dialog-name>ATC-MPinMsg</dialog-name></binding>
      <binding>
        <command>nasal</command>
        <script>globals.ATC.step_target(-1);</script>
      </binding>
    </action>

    <action>
      <name>toggle tracking</name>
      <type>adjust</type>
      <button>0</button>
      <x>109</x>
      <y>-216</y>
      <w>20</w>
      <h>20</h>
      <binding><command>dialog-close</command><dialog-name>ATC-MPinMsg</dialog-name></binding>
      <binding>
        <command>nasal</command>
        <script>globals.ATC.toggle_tracking();</script>
      </binding>
    </action>

    <action>
      <name>next mp</name>
      <type>adjust</type>
      <button>0</button>
      <x>109</x>
      <y>-238</y>
      <w>20</w>
      <h>20</h>
      <binding><command>dialog-close</command><dialog-name>ATC-MPinMsg</dialog-name></binding>
      <binding>
        <command>nasal</command>
        <script>globals.ATC.step_target(1);</script>
      </binding>
    </action>

    <action>
      <name>radar down</name>
      <type>adjust</type>
      <button>0</button>
      <x>109</x>
      <y>-159</y>
      <w>20</w>
      <h>20</h>
      <binding><command>dialog-close</command><dialog-name>ATC-MPinMsg</dialog-name></binding>
      <binding>
        <command>nasal</command>
        <script>
          globals.ATC.step_radar_range(-1);
          setprop("sim/atc/localizer-offset-display",(getprop("/sim/atc/localizer-offset"))/(getprop("/instrumentation/radar/range")))
        </script>
      </binding>
    </action>

    <action>
      <name>radar up</name>
      <type>adjust</type>
      <button>0</button>
      <x>109</x>
      <y>-137</y>
      <w>20</w>
      <h>20</h>
      <binding><command>dialog-close</command><dialog-name>ATC-MPinMsg</dialog-name></binding>
      <binding>
        <command>nasal</command>
        <script>
          globals.ATC.step_radar_range(1);
          setprop("sim/atc/localizer-offset-display",(getprop("/sim/atc/localizer-offset"))/(getprop("/instrumentation/radar/range")))
        </script>
      </binding>
    </action>

    <action>
      <name>Toggle Localizer Guideline</name>
      <type>adjust</type>
      <button>0</button>
      <x>336</x>
      <y>-154</y>
      <w>20</w>
      <h>20</h>
<!--      <binding><command>dialog-close</command><dialog-name>ATC-MPinMsg</dialog-name></binding>	-->
      <binding>
        <command>nasal</command><script>globals.ATC.toggle_localizer();</script></binding>
				<binding><command>dialog-update</command><dialog-name>ATC-MPinMsg</dialog-name></binding>
    </action>

    <action>
      <name>Offset Localizer Left</name>
      <type>adjust</type>
      <button>0</button>
      <repeatable>true</repeatable>
      <x>313</x>
      <y>-154</y>
      <w>20</w>
      <h>20</h>
<!--      <binding><command>dialog-close</command><dialog-name>ATC-MPinMsg</dialog-name></binding>	-->
      <binding>
        <condition>
          <greater-than>
            <property>sim/atc/localizer-offset</property>
            <value>-220</value>
          </greater-than>
        </condition>
        <command>nasal</command>
        <script>
          setprop("sim/atc/localizer-offset", getprop("sim/atc/localizer-offset")-1);
          setprop("sim/atc/localizer-offset-display",(getprop("/sim/atc/localizer-offset"))/(getprop("/instrumentation/radar/range")));
        </script>
      	</binding>
				<binding><command>dialog-update</command><dialog-name>ATC-MPinMsg</dialog-name></binding>
    </action>


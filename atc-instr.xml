<?xml version="1.0"?>

<PropertyList>

  <name>ATC</name>
  <w-base>1024</w-base>
  <h-base>768</h-base>
  <layers>
    <layer>
      <name>atc panel</name>
      <texture>
        <path>Aircraft/ATC-ML/panel-bg.rgb</path>
        <x1>0</x1>
        <y1>0</y1>
        <x2>1</x2>
        <y2>0.748</y2>
      </texture>
      <w>1024</w>
      <h>768</h>
    </layer>

    <layer>
      <name>MP callsign</name>
      <type>text</type>
      <condition>
        <greater-than>
          <property>/sim/atc/target-number</property>
          <value>-1</value>
        </greater-than>
      </condition>
      <font>default</font>
      <point-size>12</point-size>
      <color>
        <red>0.8</red>
        <green>0.8</green>
        <blue>0.8</blue>
      </color>
      <chunks>
        <chunk>
          <type>text-value</type>
          <property>/sim/atc/target-id</property>
        </chunk>
      </chunks>
      <transformations>
        <transformation>
          <type>x-shift</type>
          <offset>206</offset>
        </transformation>
        <transformation>
          <type>y-shift</type>
          <offset>-180</offset>
        </transformation>
      </transformations>
    </layer>

    <layer>
      <name>Tracking Indicator</name>
      <type>text</type>
      <condition>
        <property>/sim/atc/tracking</property>
      </condition>
      <font>default</font>
      <point-size>36</point-size>
        <color>
          <red>1</red>
          <green>0.3</green>
          <blue>0.3</blue>
        </color>
        <chunks>
          <chunk>
            <type>literal</type>
            <text>.</text>
          </chunk>
        </chunks>
        <transformations>
          <transformation>
            <type>x-shift</type>
            <offset>103</offset>
          </transformation>
          <transformation>
            <type>y-shift</type>
            <offset>-211</offset>
          </transformation>
        </transformations>
      </layer>

    <layer>
      <name>Loc Indicator</name>
      <type>text</type>
      <condition>
        <property>/sim/atc/localizer-display</property>
      </condition>
      <font>default</font>
      <point-size>36</point-size>
        <color>
          <red>1</red>
          <green>0.3</green>
          <blue>0.3</blue>
        </color>
        <chunks>
          <chunk>
            <type>literal</type>
            <text>.</text>
          </chunk>
        </chunks>
        <transformations>
          <transformation>
            <type>x-shift</type>
            <offset>329</offset>
          </transformation>
          <transformation>
            <type>y-shift</type>
            <offset>-149</offset>
          </transformation>
        </transformations>
      </layer>

    <layer>
      <name>MP altitude</name>
      <type>text</type>
      <condition>
        <greater-than>
          <property>/sim/atc/target-number</property>
          <value>-1</value>
        </greater-than>
      </condition>
      <font>default</font>
      <point-size>12</point-size>
        <color>
          <red>0.8</red>
          <green>0.8</green>
          <blue>0.8</blue>
        </color>
        <chunks>
          <chunk>
            <type>number-value</type>
            <property>/sim/atc/target-alt</property>
            <format>%- 5.0f</format>
          </chunk>
        </chunks>
        <transformations>
          <transformation>
            <type>x-shift</type>
            <offset>199</offset>
          </transformation>
          <transformation>
            <type>y-shift</type>
            <offset>-201</offset>
          </transformation>
        </transformations>
      </layer>

      <layer>
        <name>MP range</name>
        <type>text</type>
        <condition>
          <greater-than>
            <property>/sim/atc/target-number</property>
            <value>-1</value>
          </greater-than>
        </condition>
      <font>default</font>
      <point-size>12</point-size>
      <color>
        <red>0.8</red>
        <green>0.8</green>
        <blue>0.8</blue>
      </color>
      <chunks>
        <chunk>
          <type>number-value</type>
          <property>/sim/atc/target-range</property>
          <format>%- 3.2f</format>
        </chunk>
      </chunks>
      <transformations>
        <transformation>
          <type>x-shift</type>
          <offset>199</offset>
        </transformation>
        <transformation>
          <type>y-shift</type>
          <offset>-246</offset>
        </transformation>
      </transformations>
    </layer>

    <layer>
      <name>MP airspeed</name>
      <type>text</type>
      <condition>
        <greater-than>
          <property>/sim/atc/target-number</property>
          <value>-1</value>
        </greater-than>
